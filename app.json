{
  "expo": {
    "name": "artesanos-app",
    "slug": "artesanos-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo-icch.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "gestionaArtesanos",
    "splash": {
      "image": "./src/assets/images/logo-icch.png",
      "resizeMode": "contain",
      "backgroundColor": "#B6CD59"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
